msgid "gzip (<file>.tar.gz</file>, <file>.tgz</file>)"
msgstr "gzip (<file>.tar.gz</file>, <file>.tgz</file>)"

#: C/supported-formats.page:36(p)
msgid "bzip (<file>.tar.bz</file>, <file>.tbz</file>)"
msgstr "bzip (<file>.tar.bz</file>, <file>.tbz</file>)"

