msgstr "bzip (<file>.tar.bz</file>, <file>.tbz</file>)"

#: C/supported-formats.page:43(item/p)
msgid "bzip2 (<file>.tar.bz2</file>, <file>.tbz2</file>)"
msgstr "bzip2 (<file>.tar.bz2</file>, <file>.tbz2</file>)"

