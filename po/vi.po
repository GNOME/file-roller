msgid "Use mime icons"
msgstr "Dùng biểu tượng MIME"

#: ../data/org.gnome.FileRoller.gschema.xml.in.h:29
#| msgid "View the statusbar"
msgid "View statusbar"
msgstr "Xem thanh trạng thái"

